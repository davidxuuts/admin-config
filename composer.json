{
    "name": "davidxu/yii2-admin-config",
    "description": "This is a common library with set of foundation classes and components used by Yii2-AIO extensions by David Xu.",
    "type": "yii2-extension",
    "license": "MIT",
    "keywords": [
        "yii2 extension",
        "common library",
        "yii2"
    ],
    "authors": [
        {
            "name": "David Xu",
            "email": "david.xu.uts@163.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "davidxu/yii2-base": "^1.0.1",
        "davidxu/yii2-adminlte3-asset": "^1.0",
        "sam-it/yii2-mariadb": "^3.1",
        "ramsey/uuid": "^4.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "repositories":[
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "davidxu\\config\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
